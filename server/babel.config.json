{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "node": "current" }, "modules": false }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@": "./src"
        }
      }
    ]
  ]
}
